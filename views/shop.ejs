    <div class="page-heading bg-light">
        <div class="container">
            <div class="row align-items-end text-center">
                <div class="col-lg-7 mx-auto">
                    <h1>Shop</h1>
                    <p class="mb-4"><a href="index.html">Trang chủ</a> / <strong>Shop</strong></p>
                </div>
            </div>
        </div>
    </div>

    <div class="untree_co-section pt-3">
        <div class="container">
            <div class="row align-items-center mb-5">
                <div class="col-lg-8">
                    <h2 class="mb-3 mb-lg-0">Sản phẩm</h2>
                </div>
                <div class="col-lg-4">
                    <div class="d-flex sort align-items-center justify-content-lg-end">
                        <strong class="mr-3">Lọc theo:</strong>
                        <form action="#">
                            <select id="sort-select" required>
                                <option value="">Giá giảm dần</option>
                                <option value="1">Giá tăng dần</option>
                            </select>
                        </form>
                    </div>

                </div>
            </div>

            <div class="row">
                <div class="col-md-3">
                    <ul class="list-unstyled categories">
                        <% for (let c of loaiDh) { %>
                        <li><a href="/cat/<%=c.id%>"><%=c.name%><span>5</span></a></li>
                        <% } %>
                        
                    </ul>
                </div>
                <div class="col-md-9">
                    <div class="row product-main">
                        <% for (let w of list ) { %>
                        <div class="col-6 col-sm-6 col-md-6 mb-4 col-lg-4">
                                <div class="product-item">
                                    <a href="/watch/<%= w.id %>" class="product-img">
                                        <img src="/<%=w.url_hinh%>" alt="Image" class="img-fluid">
                                    </a>
                                    <h3 class="title"><%=w.name%></h3>
                                    <div class="price">
                                        <span><%=w.price%></span>
                                    </div>
                                </div>
                        </div>
                        <% } %>
                    </div>
                    <div class="row mt-5 pb-5">
                        <div class="col-lg-12">
                            <div class="custom-pagination">
                                <ul class="list-unstyled">
                                    <li>
                                        <a href="#">
                                            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-left"
                                                fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                                <path fill-rule="evenodd"
                                                    d="M5.854 4.646a.5.5 0 0 1 0 .708L3.207 8l2.647 2.646a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 0 1 .708 0z" />
                                                <path fill-rule="evenodd"
                                                    d="M2.5 8a.5.5 0 0 1 .5-.5h10.5a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z" />
                                            </svg>
                                        </a>
                                    </li>
                                    <li class="active"><span>1</span></li>
                                    <li><a href="#">2</a></li>
                                    <li><a href="#">3</a></li>
                                    <li><a href="#">4</a></li>
                                    <li><a href="#">5</a></li>
                                    <li>
                                        <a href="#">
                                            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-right"
                                                fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                                                <path fill-rule="evenodd"
                                                    d="M10.146 4.646a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L12.793 8l-2.647-2.646a.5.5 0 0 1 0-.708z" />
                                                <path fill-rule="evenodd"
                                                    d="M2 8a.5.5 0 0 1 .5-.5H13a.5.5 0 0 1 0 1H2.5A.5.5 0 0 1 2 8z" />
                                            </svg>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="sevenf bg-light">
            <div class="container">
                <div class="row align-items-stretch">
                    <div class="col-12 col-sm-6 col-md-4 mb-3 mb-md-0">
